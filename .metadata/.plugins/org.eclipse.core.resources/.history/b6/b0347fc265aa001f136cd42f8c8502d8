package com.financeflow.DTO;

import com.financeflow.model.UserAccount;

public class UserDTO {
	
	private Long id;
	private String name;
	private String email;
	
	public UserDTO() {
	}

	public UserDTO(Long id, String name, String email) {
		this.id = id;
		this.name = name;
		this.email = email;
	}
	
	public UserDTO(UserAccount user) {
		id = user.getId();
		name = user.getName();
		email = user.getEmail();
	}
	
	
}
